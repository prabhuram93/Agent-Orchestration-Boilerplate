{
  "name": "boilerplate-m2-analysis-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "types": "yarn wrangler types",
    "dev": "yarn types && vite dev",
    "build:staging": "yarn types && tsc -b && CLOUDFLARE_ENV=staging vite build --mode staging",
    "build:production": "yarn types && tsc -b && CLOUDFLARE_ENV=production vite build --mode production",
    "preview": "vite preview",
    "deploy:staging": "CLOUDFLARE_ENV=staging yarn wrangler deploy",
    "deploy:production": "CLOUDFLARE_ENV=production yarn wrangler deploy",
    "lint": "eslint ."
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.45.0",
    "add": "^2.0.6",
    "react": "^19.1.1",

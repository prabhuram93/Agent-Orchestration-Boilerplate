{
  "name": "boilerplate-m2-analysis",
  "main": "src/index.ts",
  "compatibility_date": "2025-05-06",
  "compatibility_flags": ["nodejs_compat"],
  "vars": {
    "ENV": "local"
  },
  "containers": [
    {
      "class_name": "Sandbox",
      "image": "./Dockerfile",
      "instance_type": "basic"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "class_name": "Sandbox",
        "name": "Sandbox"
      }
    ]
  },
  "migrations": [
    {
      "new_sqlite_classes": ["Sandbox"],
      "tag": "v1"
    }
  ],
  "r2_buckets": [
    {
      "binding": "Uploads",
      "bucket_name": "code-analysis-zip-uploads"
    }
  ],
  "observability": { "enabled": true }
}

